{
  "name": "easy-git-explorer",
  "version": "0.2.0",
  "description": "Easily explore your git repositories.",
  "homepage": "https://github.com/danisss9/easy-git-explorer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/danisss9/easy-git-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/danisss9/easy-git-explorer/issues"
  },
  "bin": {
    "easy-git-explorer": "bin/index.js"
  },
  "type": "module",
  "license": "MIT",
  "keywords": [
    "node",
    "nodejs",
    "npm",
    "npx",
    "easy",
    "visual",
    "browser",
    "angular",
    "ng",
    "react",
    "vue",
    "svelte",
    "javascript",
    "typescript",
    "js",
    "ts"
  ],
  "scripts": {
    "dev": "vite",
    "start": "node bin/index.js",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "vite build",
    "build:server": "tsc --project ./server/tsconfig.json",
    "build:start": "npm run build && npm run start",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "fastify": "^4.25.2",
    "open": "^10.0.3",
    "simple-git": "^3.25.0",
    "svelte-material-ui": "^7.0.0"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@smui/circular-progress": "^7.0.0",
    "@smui/data-table": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/tooltip": "^7.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.11.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
