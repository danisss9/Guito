    "publish:lib": "npm publish --access public"
  },
  "dependencies": {
    "fastify": "^4.25.2",
    "mustache": "^4.2.0",
    "open": "^10.0.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
