  },
  "type": "module",
  "license": "MIT",
  "keywords": [
    "node",
    "nodejs",
    "npm",
    "npx",
    "list",
    "la",
    "la",
    "easy",
    "dep",
    "circular",
    "dependencies",
    "dependency",
